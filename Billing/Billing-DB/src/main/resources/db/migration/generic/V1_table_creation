create table public.ACCOUNT (
    ID BIGINT NOT NULL,
    NUMBER VARCHAR(7) NOT NULL,
    BLOCK BIGINT NOT NULL,
    BALANCE BIGINT NOT NULL,
    ID_TARIFF BIGINT NOT NULL,
    ID_PACKAGE_SMS BIGINT,
    ID_PACKAGE_CALL BIGINT,
    ID_PACKAGE_NET BIGINT,
    REMAINING_SMS BIGINT,
    REMAINING_MIN BIGINT,
    REMAINING_MB BIGINT,
    PRIMARY KEY (ID)
);

create table public.TARIFF (
    ID BIGINT NOT NULL,
    COST BIGINT NOT NULL,
    COST_SMS BIGINT NOT NULL,
    COST_MIN BIGINT NOT NULL,
    COST_MB BIGINT NOT NULL,
    INTERNATIONAL BIGINT NOT NULL,
    PRIMARY KEY (ID)
);

create table public.PACKAGE_SMS (
    ID BIGINT NOT NULL,
    COST BIGINT NOT NULL,
    MAX_SMS BIGINT NOT NULL,
    PRIMARY KEY (ID)
);

create table public.PACKAGE_CALL (
    ID BIGINT NOT NULL,
    COST BIGINT NOT NULL,
    MAX_MIN BIGINT NOT NULL,
    PRIMARY KEY (ID)
);

create table public.PACKAGE_NET (
    ID BIGINT NOT NULL,
    COST BIGINT NOT NULL,
    MAX_MB BIGINT NOT NULL,
    PRIMARY KEY (ID)
);

create table public.HISTORY (
    ID BIGINT NOT NULL,
    DATE DATE NOT NULL,
    ID_ACCOUNT BIGINT NOT NULL,
    ACTION VARCHAR(255) NOT NULL,
    PRIMARY KEY (ID)
);

create table public.ARCHIVE (
    ID BIGINT NOT NULL,
    NUMBER VARCHAR(7) NOT NULL,
    BLOCK BIGINT NOT NULL,
    BALANCE BIGINT NOT NULL,
    ID_TARIFF BIGINT NOT NULL,
    ID_PACKAGE_SMS BIGINT,
    ID_PACKAGE_CALL BIGINT,
    ID_PACKAGE_NET BIGINT,
    REMAINING_SMS BIGINT,
    REMAINING_MIN BIGINT,
    REMAINING_MB BIGINT,
    PRIMARY KEY (ID)
);